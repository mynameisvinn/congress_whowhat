    <summaries>
      <billSummaries>
        <item>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
          <versionCode>00</versionCode>
          <actionDate>2017-07-28</actionDate>
          <text><![CDATA[<p><b>Security Clearance Review Act</b></p> <p>This bill amends the Intelligence Reform and Terrorism Prevention Act of 2004 to authorize the Federal Bureau of Investigation to revoke, or prohibit the renewal of, a security clearance of a political appointee in the Executive Office of the President if necessary for national security purposes. </p>]]></text>
          <lastSummaryUpdateDate>2017-10-12T15:32:01Z</lastSummaryUpdateDate>
          <updateDate>2017-07-28T04:00:00Z</updateDate>
        </item>
      </billSummaries>

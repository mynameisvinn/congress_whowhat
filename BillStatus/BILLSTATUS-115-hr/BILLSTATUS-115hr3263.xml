    <summaries>
      <billSummaries>
        <item>
          <actionDesc>Introduced in House</actionDesc>
          <versionCode>00</versionCode>
          <lastSummaryUpdateDate>2018-02-09T17:24:28Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2017-07-17T04:00:00Z</updateDate>
          <actionDate>2017-07-17</actionDate>
          <text><![CDATA[<p>This bill extends and expands the Independence at Home demonstration program, through which comprehensive primary care services are delivered at home to Medicare beneficiaries with multiple chronic conditions.</p>]]></text>
        </item>
        <item>
          <actionDesc>Reported to House amended, Part I</actionDesc>
          <versionCode>18</versionCode>
